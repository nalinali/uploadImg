<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
	<EditText 
		android:id="@+id/uploadUrl" android:layout_width="fill_parent"
		android:layout_height="wrap_content"/>
	<Button 
		android:id="@+id/browse" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="浏览图片"/>
	<ImageView 
		android:id="@+id/img" android:layout_width="250dip" android:layout_height="250dip"/>
	<Button 
		android:id="@+id/upload" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:text="上传图片"/>
	<TextView
		android:id="@+id/result" android:layout_width="fill_parent"
		android:layout_height="wrap_content"/>
</LinearLayout>
